<h1>Search!</h1>

<form action="/users/search" method="GET">
  <div class="user-search">
    <input type="text" class="search-string">
    <ul>
      <% @users.each do |user| %>
        <li>
          <%= link_to user.username, user_url(user) %>
          <%= render "follows/form", user: user %>
        </li>
      <% end %>
    </ul>
  </div>

</form>


